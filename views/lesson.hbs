    <div id="hamburger" class="hamburger__wrapper js-hamburger d-flex d-md-none ">
        <div class="hamburger">
            <span id="first-line"></span>
            <span id="second-line"></span>
            <span id="third-line"></span>


        </div>
    </div>
</nav>

<section class="path d-flex flex-column">
    <div style="background-image: url({{lesson.bg}})" class="info h-100 d-flex flex-column justify-content-center w-100">
        <h3>{{lesson.name}}</h3>
        <p>{{lesson.description}}</p>
    </div>
</section>
<section class="steps d-flex">
    <div class="roadmap">
        <div class="preface">
            <h4>{{lesson.preName}}</h4>
            <p class="main-idea">{{lesson.preDesc}}</p>
        </div>
        <div class="">
            {{#each sylabus}}
                <div class="sylabus-week w-100 d-flex flex-column ">

                    <div class="ss">
                        <h4>{{title}}</h4>
                        <p>{{description}}</p>
                    </div>
                    {{#each lessons}}
                        <div class="w-100 d-flex align-items-center mb-4 justify-content-between">
                            <div class="circle-big d-flex align-items-center justify-content-center">
                                <div class="circle-small"></div>
                                {{#if connected}}
                                    <div class="connector"></div>
                                {{/if}}
                            </div>
                            <div class="item d-flex  align-items-center">

                                <img class="lesson-img mr-5" src="{{img}}" alt="">
                                <div class="i-tb d-flex flex-column h-100 justify-content-center">
                                    <h5>{{name}}</h5>
                                    <p>{{description}}</p>
                                    <div class="line"></div>
                                </div>
                                <div class="days d-flex flex-column justify-content-around">
                                    <ul>
                                        {{#each lesson}}
                                            <li>{{text}}</li>
                                        {{/each}}
                                    </ul>
                                </div>

                            </div>
                        </div>

                    {{/each}}
                </div>
            {{/each}}
        </div>
    </div>
    <div class="lessons d-none d-md-flex flex-column align-items-center">
        <div class="courses-type d-flex align-items-center justify-content-center flex-column">
            <div class="bezel d-flex align-items-center justify-content-center mb-5">
                <img src="{{lesson.img}}" alt="">
            </div>
            <h5>
               {{lesson.name}}
            </h5>
        </div>
        <div class="navigation-courses">
            <h5>Другие курсы:</h5>
            <ul>
                <li>
                    <a href="/lesson/html">
                        HTML/CSS
                    </a>
                </li>

                <li>
                    <a href="/lesson/scratch">
                        Scratch
                    </a>
                </li>
                <li>
                    <a href="/lesson/java">
                        Java
                    </a>
                </li>

                <li>
                    <a href="/lesson/python">
                        Python
                    </a>
                </li>

                <li>
                    <a href="/lesson/algorithm">
                        Algorithm
                    </a>
                </li>
                <li>
                    <a href="/lesson/design">
                        Design
                    </a>
                </li>
                <li>
                    <a href="/lesson/web-intensive">
                        Web-intensive
                    </a>
                </li>
            </ul>

        </div>
    </div>

</section>
